	<gridbagconstraints insets="2,2,2,2" gridx="0" gridy="0"/>
	<label PreferredSize="75,20"/>
	<cscombobox initClass="CSComboBox">
		<gridbagconstraints insets="1,1,1,1" gridx="1" gridy="0" gridwidth="1"/>
	</cscombobox>
