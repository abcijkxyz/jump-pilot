<panel layout="GridBagLayout" border="EtchedBorder">
	<panel id="p_categories" layout="GridLayout(0,1)">
		<gridbagconstraints insets="1,5,1,5" gridx="0" gridy="1"/>
		<label text="Transform all layers to">
			<gridbagconstraints insets="0,5,0,5"/>
		</label>
	</panel>
	<panel id="p_layers" layout="GridLayout(0,1)">
		<gridbagconstraints insets="1,5,1,5" gridx="0" gridy="0"/>
		<label text="Assign coordinate systems to these layers:"/>
	</panel>
	<panel id="p_buttons">	
		<gridbagconstraints insets="1,2,1,2" gridx="0" gridy="2"/>
		<button text="OK" ActionCommand="AC_OK">
			<gridbagconstraints insets="1,1,1,1" gridx="0" gridy="3" fill="GridBagConstraints.NONE" anchor="GridBagConstraints.EAST" weightx="1.0"/>
		</button>
	</panel>
</panel>